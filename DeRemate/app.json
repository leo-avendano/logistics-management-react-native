{
  "expo": {
    "name": "DeRemate",
    "slug": "DeRemate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/DePedidosLogo.png",
    "scheme": "deremate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/DePedidosLogo.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "Nueva Ruta Disponible",
      "iosDisplayInForeground": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.logistics_management_react_native.DeRemate",
      "permissions": [
        "CAMERA",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/DePedidosLogo.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "919e7ead-d58f-411b-b0ee-1cd5cc65b264"
      }
    }
  }
}
